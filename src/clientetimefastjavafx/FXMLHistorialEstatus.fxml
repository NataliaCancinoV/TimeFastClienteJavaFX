            </ImageView>
         </children>
      </Pane>
      <Pane layoutX="50.0" layoutY="107.0" onMouseClicked="#regresarEnvios" prefHeight="200.0" prefWidth="316.0" styleClass="apTimeFastTitulo" stylesheets="@fxmlhistorialestatus.css">

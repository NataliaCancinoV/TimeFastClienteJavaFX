            </TableView>
         </content>
      </ScrollPane>
      <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="323.0" layoutY="206.0" onMouseClicked="#obtenerEstatusNoGuia" pickOnBounds="true" preserveRatio="true">

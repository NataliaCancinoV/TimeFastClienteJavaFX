            <TextField fx:id="tfBuscar" layoutX="24.0" layoutY="12.0" promptText="Buscar" styleClass="tfTimeFastBusqueda" stylesheets="@fxmlunidades.css" />
            <ImageView fitHeight="36.0" fitWidth="34.0" layoutX="281.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#buscarUnidad">
               <image>
                  <Image url="@recursos/search.png" />
               </image>
            </ImageView>
             <Button layoutX="571.0" layoutY="14.0" mnemonicParsing="false" onAction="#irPantallaHistorial" prefHeight="39.0" prefWidth="150.0" text="Historial" />
            <Button layoutX="758.0" layoutY="11.0" mnemonicParsing="false" onAction="#btnElminarUnidad" prefHeight="39.0" prefWidth="150.0" styleClass="btnTimeFastSecundario" stylesheets="@fxmlunidades.css" text="  Eliminar  ">
